{
  "name": "exercise-tracker",
  "version": "1.0.0",
  "description": "An app for tracking workouts, exercises, and progress",
  "main": "src/server.ts",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev-client": "npm run build --prefix client",
    "dev-server-watch": "tsc -w",
    "dev-server-start": "nodemon ./build/src/server.js",
    "dev": "concurrently npm:dev-*",
    "server": "concurrently npm:dev-server-*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm install --prefix client",
    "deploy": "git push heroku",
    "heroku-postbuild": "npm run build --prefix client && echo \"Client built\" && tsc && chmod 555 ./build/src/server.js"
  },
  "author": "David Taylor",
  "license": "ISC",
  "dependencies": {
    "@codewizard-dt/react-semantic-datetime-picker": "0.0.20",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.4"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
  }
}